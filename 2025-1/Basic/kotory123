#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(NULL);

    int a,b,c,d;
    int arr[10] = { 0 };
    
      cin >> a >> b >> c;
      int sum = a * b * c;
     
   while(sum!=0){
        d = sum % 10;
        arr[d]++;
        sum /= 10;

        }
  
    for (int i = 0;i < 10;i++) {
        cout << arr[i] << endl;
    }
}
